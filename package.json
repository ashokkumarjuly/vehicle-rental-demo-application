{
    "name": "api-server",
    "version": "1.0.0",
    "description": "Restfull API build usng express and typescript",
    "engines": {
        "node": ">=14",
        "npm": ">=6.9.0"
    },
    "main": "./src/server.js",
    "scripts": {
        "start": "cross-env NODE_ENV=production ./src/server.js -i",
        "start:dev": "cross-env NODE_ENV=development ./src/server.js -i",
        "build": "cross-env NODE_ENV=production gulp",
        "build:dev": "cross-env NODE_ENV=development gulp",
        "local": "cross-env NODE_ENV=local ts-node-dev  --respawn --transpile-only --watch \"src/**\" --ignore-watch=\"['src/**/*.json']\" ./src/server.ts ",
        "compileTs": "gulp compileTs",
        "test": "npm run test:all",
        "test:all": "npm run test:unit && npm run test:integration",
        "test:unit": "cross-env NODE_ENV=testing DB_LOG_QUERY=false ENABLE_DEBUG=false node --trace-warnings ./node_modules/jest/bin/jest --detectOpenHandles --runInBand --no-coverage",
        "test:unit:watch": "cross-env DB_LOG_QUERY=false node --trace-warnings ./node_modules/jest/bin/jest --detectOpenHandles --onlyChanged --runInBand --no-coverage --colors --watch",
        "test:integration": "cross-env NODE_ENV=testing jest --runInBand --testTimeout=10000 -c jest.integration.config.js --detectOpenHandles --no-coverage --verbose -i",
        "coverage": "cross-env DB_LOG_QUERY=false ENABLE_DEBUG=false  jest --coverage --verbose --reporters default jest-stare",
        "lint": "eslint ./src/**/*.{ts,tsx,jsx}",
        "lint:fix": "eslint ./src/**/*.{ts,tsx,jsx} --fix",
        "prettier:check": "prettier --check src/**/*.{ts,tsx,jsx}",
        "prettier:write": "prettier --write src/**/*.{ts,tsx,jsx}",
        "prepare": "husky install",
        "lint-staged": "lint-staged"
    },
    "author": "Ashok",
    "license": "ISC",
    "dependencies": {
        "@joi/date": "^2.1.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/express-session": "^1.17.4",
        "@types/ua-parser-js": "^0.7.36",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-basic-auth": "^1.2.0",
        "express-session": "^1.17.2",
        "file-stream-rotator": "^0.5.7",
        "fs-extra": "^10.0.0",
        "helmet": "^4.6.0",
        "http-status-codes": "^2.1.4",
        "i18next": "^21.4.1",
        "i18next-fs-backend": "^1.1.1",
        "i18next-http-backend": "^1.3.1",
        "i18next-http-middleware": "^3.1.4",
        "joi": "^17.4.2",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "method-override": "^3.0.0",
        "moment": "^2.29.1",
        "morgan": "^1.10.0",
        "mysql2": "^2.3.2",
        "passport": "^0.5.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "rate-limiter-flexible": "^2.3.4",
        "sequelize": "^6.9.0",
        "source-map-support": "^0.5.20",
        "swagger-ui-express": "^4.1.6",
        "tsc-watch": "^4.5.0",
        "ua-parser-js": "^1.0.2",
        "url-parse": "^1.5.3",
        "uuid": "^8.3.2",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.5",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/body-parser": "^1.19.1",
        "@types/compression": "1.7.2",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.13",
        "@types/express-jwt": "6.0.2",
        "@types/express-serve-static-core": "^4.17.25",
        "@types/express-useragent": "^1.0.1",
        "@types/faker": "^5.5.9",
        "@types/fs-extra": "^9.0.13",
        "@types/hapi__joi": "^17.1.7",
        "@types/helmet": "0.0.48",
        "@types/jest": "^27.0.2",
        "@types/joi": "^17.2.2",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/method-override": "0.0.32",
        "@types/morgan": "^1.9.3",
        "@types/node": "^16.11.7",
        "@types/passport": "^1.0.7",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/sequelize": "^4.28.10",
        "@types/source-map-support": "^0.5.4",
        "@types/supertest": "^2.0.11",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/url-parse": "^1.4.4",
        "@types/uuid": "^8.3.1",
        "@types/validator": "^13.6.6",
        "@types/yamljs": "^0.2.31",
        "@typescript-eslint/eslint-plugin": "^5.3.1",
        "@typescript-eslint/parser": "^5.3.1",
        "cross-env": "^7.0.3",
        "del": "^6.0.0",
        "eslint": "^8.2.0",
        "eslint-config-airbnb-typescript": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-unicorn": "^38.0.1",
        "faker": "^5.5.3",
        "gulp": "^4.0.2",
        "gulp-if": "^3.0.0",
        "gulp-rename": "^2.0.0",
        "gulp-run": "^1.7.1",
        "gulp-typescript": "^5.0.1",
        "gulp-uglify": "^3.0.2",
        "husky": "^7.0.4",
        "jest": "^27.3.1",
        "jest-serial-runner": "^1.2.0",
        "jest-stare": "^2.3.0",
        "lint-staged": "^11.2.6",
        "prettier": "^2.4.1",
        "pretty-error": "^4.0.0",
        "supertest": "^6.1.6",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.4"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts}": [
            "prettier --write",
            "eslint ./src/**/*.{ts,tsx,jsx} --fix",
            "git add"
        ],
        "*.+(js|json|md)": [
            "prettier --write"
        ]
    }
}
